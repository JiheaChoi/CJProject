<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DeliveryActivity"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1.5"
        android:background="@color/red"
        android:gravity="center"
        android:text="배달 전화번호부"
        android:textColor="#ffffff"
        android:textSize="30sp"
        android:textStyle="bold" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="138dp"
        android:layout_weight="0.1"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="146dp"
            android:layout_height="115dp"
            android:layout_gravity="center"
            android:layout_marginTop="50dp"
            android:layout_marginBottom="50dp"
            app:srcCompat="@drawable/china" />

        <TextView
            android:id="@+id/txtChina"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="60dp"
            android:gravity="center"
            android:text="외백"
            android:textSize="20sp" />

        <Button
            android:id="@+id/btnChina"
            android:layout_width="47dp"
            android:layout_height="48dp"
            android:layout_marginLeft="50dp"
            android:background="@drawable/call" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="97dp"
        android:layout_weight="1"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/kim"
            android:layout_width="159dp"
            android:layout_height="102dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/kim" />

        <TextView
            android:id="@+id/txtKim"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="50dp"
            android:gravity="center"
            android:text="김가네"
            android:textSize="20sp" />

        <Button
            android:id="@+id/btnKim"
            android:layout_width="47dp"
            android:layout_height="48dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/call" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/bhc"
            android:layout_width="176dp"
            android:layout_height="113dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/bhc" />

        <TextView
            android:id="@+id/txtBhc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="50dp"
            android:text="BHC 치킨"
            android:textSize="20sp" />

        <Button
            android:id="@+id/btnBhc"
            android:layout_width="47dp"
            android:layout_height="48dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/call" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/sin"
            android:layout_width="176dp"
            android:layout_height="113dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/sin" />

        <TextView
            android:id="@+id/txtSin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="50dp"
            android:text="신전떡볶이"
            android:textSize="20sp" />

        <Button
            android:id="@+id/btnSin"
            android:layout_width="47dp"
            android:layout_height="48dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/call" />
    </LinearLayout>
    <TextView
        android:id="@+id/textView10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="2" />


</LinearLayout>